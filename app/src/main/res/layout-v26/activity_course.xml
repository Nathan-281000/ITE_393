<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ite393_2nd_bg"
    tools:context=".Course">

    <ScrollView
        android:id="@+id/scrollView"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_marginBottom="100dp"
        android:layout_marginTop="3dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.01"
        tools:ignore="SpeakableTextPresentCheck">

        <LinearLayout
            android:id="@+id/aboutView_2"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:textAlignment="gravity"
            android:orientation="vertical">
            <ImageView
                android:layout_marginTop="30dp"
                android:id="@+id/imageView10"
                android:layout_width="match_parent"
                android:layout_height="150dp"
                android:scaleType="fitCenter"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:srcCompat="@drawable/phinmaedlogo"
                tools:ignore="ContentDescription,ImageContrastCheck" />

            <TextView
                android:layout_gravity="center"
                android:background="@drawable/rounded_bg_for_cite"
                android:layout_width="360dp"
                android:layout_height="100dp"
                android:text="CITE"
                android:textStyle="bold"
                android:textSize="30sp"
                android:layout_marginTop="@dimen/_20dp"
                android:textColor="@color/white"
                android:fontFamily="sans-serif"
                android:gravity="center"
                tools:ignore="HardcodedText" />

            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/cite"
                android:layout_gravity="center"
                tools:ignore="ContentDescription" />

            <TextView
                android:id="@+id/frag_txt"
                android:layout_width="260dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:padding="6dp"
                android:text="@string/citetxt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="TextSizeCheck" />

            <TextView
                android:id="@+id/citeTxt"
                android:layout_gravity="center"
                android:padding="10dp"
                android:justificationMode="inter_word"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:paddingTop="5dp"
                android:text="@string/citeTXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="19sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

            <TextView
                android:layout_gravity="center"
                android:background="@drawable/round_bg_for_cahs"
                android:layout_width="360dp"
                android:layout_height="100dp"
                android:text="CAHS"
                android:textStyle="bold"
                android:textSize="30sp"
                android:layout_marginTop="@dimen/_20dp"
                android:textColor="@color/black"
                android:fontFamily="sans-serif"
                android:gravity="center"
                tools:ignore="HardcodedText" />

            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:src="@drawable/mls"
                tools:ignore="ContentDescription,ImageContrastCheck" />

            <TextView
                android:layout_width="260dp"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:padding="6dp"
                android:text="@string/cahstxt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold"
                tools:ignore="TextContrastCheck" />

            <TextView
                android:id="@+id/cahsTxt"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:justificationMode="inter_word"
                android:padding="10dp"
                android:paddingTop="5dp"
                android:text="@string/cahsTXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="19sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

            <TextView
                android:layout_gravity="center"
                android:background="@drawable/round_bg_for_cea"
                android:layout_width="360dp"
                android:layout_height="100dp"
                android:text="CEA"
                android:textStyle="bold"
                android:textSize="30sp"
                android:layout_marginTop="@dimen/_20dp"
                android:textColor="@color/black"
                android:fontFamily="sans-serif"
                android:gravity="center"
                tools:ignore="HardcodedText" />

            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:layout_gravity="center"
                android:src="@drawable/cea"
                tools:ignore="ContentDescription,ImageContrastCheck" />
            <TextView
                android:id="@+id/satxt"
                android:layout_width="210dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:padding="6dp"
                android:gravity="center"
                android:text="@string/cea_BSA_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/saTXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:padding="10dp"
                android:justificationMode="inter_word"
                android:gravity="center"
                android:paddingTop="5dp"
                android:text="@string/bsa_TXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:layout_marginBottom="50dp"
                android:textSize="19sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />


            <TextView
                android:id="@+id/cetxt"
                android:layout_width="210dp"
                android:padding="6dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="@string/cea_BSCE_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/ceTXT"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:justificationMode="inter_word"
                android:padding="10dp"
                android:paddingTop="5dp"
                android:text="@string/ce_TXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="19sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

            <TextView
                android:layout_gravity="center"
                android:background="@drawable/round_bg_for_cma"
                android:layout_width="360dp"
                android:layout_height="100dp"
                android:text="CMA"
                android:textStyle="bold"
                android:textSize="30sp"
                android:layout_marginTop="@dimen/_20dp"
                android:textColor="@color/black"
                android:fontFamily="sans-serif"
                android:gravity="center"
                tools:ignore="HardcodedText" />

            <ImageView
                android:layout_width="150dp"
                android:layout_height="150dp"
                android:src="@drawable/cma"
                android:layout_gravity="center"
                tools:ignore="ContentDescription" />
            <TextView
                android:id="@+id/SA"
                android:layout_width="220dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:padding="6dp"
                android:gravity="center"
                android:text="@string/cma_SA_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/SA_txt"
                android:padding="10dp"
                android:justificationMode="inter_word"
                android:layout_gravity="center"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:paddingTop="5dp"
                android:text="@string/SA_TXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:layout_marginBottom="50dp"
                android:textSize="19sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />


            <TextView
                android:id="@+id/AT"
                android:layout_width="220dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:padding="6dp"
                android:gravity="center"
                android:text="@string/cma_AT_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/AT_txt"
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:justificationMode="inter_word"
                android:padding="10dp"
                android:paddingTop="5dp"
                android:text="@string/AT_THXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="19sp"
                android:layout_marginBottom="50dp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

            <TextView
                android:id="@+id/TM"
                android:layout_width="220dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:padding="6dp"
                android:gravity="center"
                android:text="@string/cma_TM_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/tm_txt"
                android:layout_width="match_parent"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:justificationMode="inter_word"
                android:padding="10dp"
                android:gravity="center"
                android:paddingTop="5dp"
                android:text="@string/TM_TXT"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="19sp"
                android:layout_marginBottom="50dp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

            <TextView
                android:id="@+id/ENTREP"
                android:layout_width="220dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:text="@string/cma_ENTREP_txt"
                android:textAlignment="center"
                android:textColor="@color/black"
                android:padding="6dp"
                android:textSize="20sp"
                android:textStyle="bold" />

            <TextView
                android:id="@+id/entrep_txt"
                android:layout_width="match_parent"
                android:justificationMode="inter_word"
                android:padding="10dp"
                android:layout_gravity="center"
                android:layout_height="wrap_content"
                android:backgroundTint="#E7EFE6"
                android:fontFamily="sans-serif"
                android:gravity="center"
                android:paddingTop="5dp"
                android:text="@string/the_bachelor_of_science_in_entrepreneurship_bs_entrepreneurship_is_a_dynamic_program_that_equips_students_with_essential_knowledge_and_skills_for_success_in_entrepreneurship_the_curriculum_integrates_theory_with_hands_on_experience_covering_business_planning_financial_management_marketing_and_innovation_through_real_world_projects_graduates_emerge_ready_to_identify_opportunities_create_sustainable_business_models_and_navigate_the_challenges_of_entrepreneurship_the_program_fosters_creativity_and_resilience_preparing_students_to_excel_as_business_leaders_and_innovators"
                android:textAlignment="center"
                android:textColor="#373333"
                android:textSize="18sp"
                android:textStyle="italic"
                tools:ignore="VisualLintLongText" />

        </LinearLayout>
    </ScrollView>
    <TableRow
        android:id="@+id/tableRow2"
        android:layout_width="match_parent"
        android:layout_height="99dp"
        android:layout_marginLeft="10dp"
        android:layout_marginRight="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/bg_corners"
        android:backgroundTint="#D1C3C3"
        android:scaleType="centerInside"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/scrollView2"
        app:layout_constraintVertical_bias="1.0"
        tools:ignore="UnknownIdInLayout">

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">

            <ImageButton
                android:id="@+id/coursebtn"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:backgroundTint="#00FFFFFF"
                android:contentDescription="@string/app_name"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/course"
                tools:ignore="DuplicateSpeakableTextCheck" />
            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Course"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.100"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.975"
                tools:ignore="HardcodedText" />
        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">

            <ImageButton
                android:id="@+id/modules"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:backgroundTint="#00FFFFFF"
                android:contentDescription="@string/app_name"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/modules"
                tools:ignore="ImageContrastCheck" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:gravity="center"
                android:text="Modalities and Student Handbook"
                android:textColor="@color/black"
                android:textStyle="bold"
                android:textSize="11sp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.365"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.975"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center">

            <ImageButton
                android:id="@+id/maps"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:backgroundTint="#00FFFFFF"
                android:contentDescription="@string/app_name"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/maps"
                tools:ignore="ImageContrastCheck" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Map"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.625"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.975"
                tools:ignore="HardcodedText" />

        </LinearLayout>

        <LinearLayout
            android:layout_weight="1"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical"
            android:gravity="center"
            tools:ignore="VisualLintBounds">
            <ImageButton
                android:id="@+id/profile"
                android:layout_width="50dp"
                android:layout_height="50dp"
                android:layout_marginLeft="20dp"
                android:layout_marginRight="20dp"
                android:backgroundTint="#00FFFFFF"
                android:contentDescription="@string/app_name"
                android:scaleType="centerInside"
                app:srcCompat="@drawable/pogiko"
                tools:ignore="DuplicateSpeakableTextCheck" />

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Profile"
                android:textColor="@color/black"
                android:textStyle="bold"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintHorizontal_bias="0.890"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.975"
                tools:ignore="HardcodedText" />
        </LinearLayout>
    </TableRow>
</androidx.constraintlayout.widget.ConstraintLayout>